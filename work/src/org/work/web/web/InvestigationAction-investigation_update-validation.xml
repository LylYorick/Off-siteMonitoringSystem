<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.dtd">
<validators>
	<!-- 协查名单 -->
	<!-- 协查函号 -->
	<field name="checkid">
		<field-validator type="requiredstring">
			<message key="FieldRequired"/>
		</field-validator>		
	</field>
	  
	<!-- 可疑交易描述 -->
	<field name="description">
		<field-validator type="requiredstring">
			<message key="FieldRequired"/>
		</field-validator>		
	</field>
	
	<!-- 协查内容 -->
	<field name="content">
		<field-validator type="requiredstring">
			<message key="FieldRequired"/>
		</field-validator>		
	</field>
	
	<!-- 发函时间 -->
	<field name="senddate">
		<field-validator type="requiredstring">
			<message key="FieldRequired"/>
		</field-validator>		
	</field>
	
	<!-- 协查单位 -->
	<field name="unit">
		<field-validator type="requiredstring">
			<message key="FieldRequired"/>
		</field-validator>		
	</field>
	
	<!-- 经手人 -->
	<field name="handledby">
		<field-validator type="requiredstring">
			<message key="FieldRequired"/>
		</field-validator>		
	</field>

	<!-- 实际回函时间 -->
	<field name="backdate">
		<field-validator type="requiredstring">
			<message key="FieldRequired"/>
		</field-validator>		
	</field>
	
	<!-- 主体名称 -->
	<field name="subjectname">
		<field-validator type="requiredstring">
			<message key="FieldRequired"/>
		</field-validator>		
	</field>
	
	<!-- 证件号 -->
	<field name="idnumber">
		<field-validator type="requiredstring">
			<message key="FieldRequired"/>
		</field-validator>		
	</field>
	
	<!-- 备注 -->
	<field name="note">
		<field-validator type="requiredstring">
			<message key="FieldRequired"/>
		</field-validator>		
	</field>
</validators>