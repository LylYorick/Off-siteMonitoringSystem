<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.dtd">
<validators>
	<!-- 培训时间 -->
	<field name="train.trdate">
		<field-validator type="requiredstring">
			<message key="FieldRequired"/>
		</field-validator>
	</field>
	<!-- 培训内容 -->
	<field name="train.trcnt">
		<field-validator type="requiredstring">
			<message key="FieldRequired"/>
		</field-validator>
	</field>
	<!-- 培训对象 -->
	<field name="train.trobjt">
		<field-validator type="requiredstring">
			<message key="FieldRequired"/>
		</field-validator>
	</field>
	<!-- 参加人数 -->
	<field name="train.trnum">
		<field-validator type="requiredstring">
			<message key="FieldRequired"/>
		</field-validator>
	</field>
	<!-- 培训方式 -->
	<field name="train.trmethod">
		<field-validator type="requiredstring">
			<message key="FieldRequired"/>
		</field-validator>
	</field>
</validators>