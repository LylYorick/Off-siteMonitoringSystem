<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
	<class name="org.work.web.po.Rate" table="ORG_RATE">
		<id name="id" column="ID"   type="java.lang.Integer"  unsaved-value="0">
				<generator class="increment"></generator>
		</id>
		<property name="year" column="ANNUAL"   type="java.lang.String"  length="4" >
		</property>
		<property name="orgId" column="ORG_ID"   type="java.lang.Integer">
		</property>
		<property name="orgType" column="ORG_TYPE"   type="java.lang.Integer">
		</property>
		<property name="rateType" column="RATE_TYPE"   type="java.lang.Integer">
		</property>
		<property name="status" column="STATUS"   type="java.lang.String"  length="2" >
		</property>
		<property name="saTatolScore" column="SA_TATOL_SCORE"   type="java.lang.Integer">
		</property>
		<property name="saScore" column="SA_SCORE"   type="java.lang.Integer">
		</property>
		<property name="rateLevel" column="RATE_LEVEL"   type="java.lang.String"  length="3" >
		</property>
		<property name="rateScore" column="RATE_SCORE"   type="java.lang.Integer">
		</property>
		<property name="rankLevel" column="RANK_LEVEL"   type="java.lang.String"  length="3" >
		</property>
		<property name="rankReason" column="RANK_REASON"   type="java.lang.String"  length="1000" >
		</property>
		<property name="isOpposition" column="IS_OPPOSITION"   type="java.lang.String"  length="2" >
		</property>
		<property name="opinionBook" column="OPINION_BOOK"   type="java.lang.String"  length="500" >
		</property>
		<property name="bookStatus" column="BOOK_STATUS"   type="java.lang.String"  length="2" >
		</property>
		<property name="rectificationReport" column="RECTIFICATION_REPORT"   type="java.lang.String"  length="500" >
		</property>
		<property name="reportType" column="REPORT_TYPE"   type="java.lang.String"  length="2" >
		</property>
		<property name="reportStatus" column="REPORT_STATUS"   type="java.lang.String"  length="4" >
		</property>
	</class>
</hibernate-mapping>
