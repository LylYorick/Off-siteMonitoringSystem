<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.work.web.po" auto-import="true">
	<class name="StatusControl" table="RATE_STATUS_CONTROL">
		<meta attribute="class-description">RATE_STATUS_CONTROL</meta>
		<meta attribute="class-gb2312-name">RATE_STATUS_CONTROL</meta>
			<id name="id" column="ID"   type="java.lang.Integer">
				<generator class="increment"></generator>
			</id>
		<property name="year" column="ANNUAL"   type="java.lang.String" length="4">
		</property>
		<property name="type" column="STATUS_TYPE"   type="java.lang.String" length="2">
		</property>
		<property name="name" column="STATUS_NAME"   type="java.lang.String" length="50">
		</property>
		<property name="startTime" column="START_TIME"   type="java.lang.String" length="20">
		</property>
		<property name="endTime" column="END_TIME"   type="java.lang.String" length="20">
		</property>
		<property name="activeStatus" column="ACTIVE_STATUS"   type="java.lang.String"  length="2">
		</property>
		<property name="remark" column="REMARK"   type="java.lang.String"  length="1000">
		</property>
	</class>
</hibernate-mapping>
